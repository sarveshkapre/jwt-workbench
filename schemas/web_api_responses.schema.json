{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://jwt-workbench.invalid/schemas/web_api_responses.schema.json",
  "title": "JWT Workbench Web API Responses",
  "type": "object",
  "additionalProperties": true,
  "$defs": {
    "JwtHeader": {
      "type": "object",
      "additionalProperties": true
    },
    "JwtPayload": {
      "type": "object",
      "additionalProperties": true
    },
    "Warnings": {
      "type": "array",
      "items": { "type": "string" }
    },
    "ErrorResponse": {
      "type": "object",
      "required": ["error"],
      "properties": {
        "error": { "type": "string" }
      },
      "additionalProperties": false
    },
    "DecodeResponse": {
      "type": "object",
      "required": ["header", "payload", "warnings"],
      "properties": {
        "header": { "$ref": "#/$defs/JwtHeader" },
        "payload": { "$ref": "#/$defs/JwtPayload" },
        "warnings": { "$ref": "#/$defs/Warnings" }
      },
      "additionalProperties": false
    },
    "VerifyResponse": {
      "type": "object",
      "required": ["header", "payload", "warnings"],
      "properties": {
        "header": { "$ref": "#/$defs/JwtHeader" },
        "payload": { "$ref": "#/$defs/JwtPayload" },
        "warnings": { "$ref": "#/$defs/Warnings" },
        "key_thumbprint_sha256": { "type": "string" }
      },
      "additionalProperties": false
    },
    "SignResponse": {
      "type": "object",
      "required": ["token", "warnings"],
      "properties": {
        "token": { "type": "string" },
        "warnings": { "$ref": "#/$defs/Warnings" }
      },
      "additionalProperties": false
    },
    "ExportResponse": {
      "type": "object",
      "required": ["token_redacted", "header", "payload", "warnings", "notes"],
      "properties": {
        "token_redacted": { "type": "string" },
        "header": { "$ref": "#/$defs/JwtHeader" },
        "payload": { "$ref": "#/$defs/JwtPayload" },
        "warnings": { "$ref": "#/$defs/Warnings" },
        "notes": { "type": "string" }
      },
      "additionalProperties": false
    },
    "SessionVerifyContext": {
      "type": "object",
      "required": ["key_type", "leeway", "allow_network"],
      "properties": {
        "alg": { "type": "string" },
        "key_type": { "type": "string" },
        "key_text": { "type": "string" },
        "kid": { "type": "string" },
        "aud": {},
        "iss": {},
        "require": {
          "type": "array",
          "items": { "type": "string" }
        },
        "leeway": { "type": "integer" },
        "at": { "type": "integer" },
        "allow_network": { "type": "boolean" },
        "jwks_url": { "type": "string" },
        "oidc_issuer": { "type": "string" },
        "jwks_cache_path": { "type": "string" }
      },
      "additionalProperties": false
    },
    "SessionBundle": {
      "type": "object",
      "required": [
        "schema_version",
        "created_at",
        "token",
        "token_redacted",
        "header",
        "payload",
        "warnings",
        "verify",
        "key_material_included"
      ],
      "properties": {
        "schema_version": { "type": "integer" },
        "created_at": { "type": "integer" },
        "token": { "type": "string" },
        "token_redacted": { "type": "string" },
        "header": { "$ref": "#/$defs/JwtHeader" },
        "payload": { "$ref": "#/$defs/JwtPayload" },
        "warnings": { "$ref": "#/$defs/Warnings" },
        "verify": { "$ref": "#/$defs/SessionVerifyContext" },
        "key_material_included": { "type": "boolean" },
        "notes": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "additionalProperties": false
    },
    "SessionExportResponse": {
      "type": "object",
      "required": ["session"],
      "properties": {
        "session": { "$ref": "#/$defs/SessionBundle" }
      },
      "additionalProperties": false
    },
    "SessionImportResponse": {
      "type": "object",
      "required": ["session"],
      "properties": {
        "session": { "$ref": "#/$defs/SessionBundle" }
      },
      "additionalProperties": false
    },
    "JwkResponse": {
      "type": "object",
      "required": ["jwk"],
      "properties": {
        "jwk": { "type": "object", "additionalProperties": true }
      },
      "additionalProperties": false
    },
    "JwksResponse": {
      "type": "object",
      "required": ["jwks"],
      "properties": {
        "jwks": {
          "type": "object",
          "required": ["keys"],
          "properties": {
            "keys": { "type": "array", "items": { "type": "object" } }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": false
    },
    "KeyPresetResponse": {
      "type": "object",
      "required": ["key_type", "key_text"],
      "properties": {
        "key_type": { "type": "string" },
        "key_text": { "type": "string" },
        "alg": { "type": "string" }
      },
      "additionalProperties": true
    },
    "SampleResponse": {
      "type": "object",
      "required": ["token", "header", "payload", "warnings", "alg", "key_type", "key_text", "require"],
      "properties": {
        "token": { "type": "string" },
        "header": { "$ref": "#/$defs/JwtHeader" },
        "payload": { "$ref": "#/$defs/JwtPayload" },
        "warnings": { "$ref": "#/$defs/Warnings" },
        "alg": { "type": "string" },
        "key_type": { "type": "string" },
        "key_text": { "type": "string" },
        "kid": { "type": "string" },
        "aud": {},
        "iss": {},
        "leeway": { "type": "integer" },
        "require": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "additionalProperties": true
    }
  }
}
